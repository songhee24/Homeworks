create table sport_types1 (
	id integer primary key,
	name varchar(50) not null,
	salary integer  not null
);
create table trainers1 (
	id integer primary key,
	name varchar(50) not null,
	phone_number varchar(20),
	inn varchar(14) unique not null,
	sport_type1 integer references sport_types1(id)
);


create table visitors(
    id integer primary key,
    name varchar(50) not null,
    birthday varchar(50) not null,
    gender varchar(50) not null
);

create table visits(
   visitors integer references visitors(id),
   time integer not null,
   trainers1 integer references trainers1(id)
);


select * from visitors;
insert into visitors (id, name,birthday,gender)
values (1, 'Alex', 'may 16th','male');

select * from trainers1;
insert into trainers1(id,name,phone_number,inn)
values (2,'trainer','1234567','abc125')

select * from visits;
insert into visits(visitors,time,trainers1)
values(1,12,2);